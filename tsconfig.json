{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "paths": {
      "@navigators": ["./src/navigators"],
      "@navigators/*": ["./src/navigators/*"],
      "@screens": ["./src/screens"],
      "@screens/*": ["./src/screens/*"],
      "@store": ["./src/store"],
      "@store/*": ["./src/store/*"],
      "@constants": ["./src/constants"],
      "@constants/*": ["./src/constants/*"],
      "@content": ["./src/content"],
      "@content/*": ["./src/content/*"],
      "@utils/*": ["./src/utils/*"]
    }
  }
}
